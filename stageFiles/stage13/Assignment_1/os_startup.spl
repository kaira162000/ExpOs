//Mod 7
loadi(54,67);
loadi(55,68);
//Idle
loadi(69,11);
loadi(70,12);

//Set stack to userAreaPage of IDLE
SP = 82*512 - 1;
call BOOT_MODULE;

//PTBR for idle
PTBR=PAGE_TABLE_BASE;  //as PID of idle process is 0
PTLR = 10;
//Library
[PTBR+0] = 63;
[PTBR+1] = "0100";
[PTBR+2] = 64;
[PTBR+3] = "0100";

//Heap
[PTBR+4] = 84;
[PTBR+5] = "0110";
[PTBR+6] = 85;
[PTBR+7] = "0110";

//Code
[PTBR+8] = 69;
[PTBR+9] = "0100";
[PTBR+10] = 70;
[PTBR+11] = "0100";
[PTBR+12] = -1;
[PTBR+13] = "0000";
[PTBR+14] = -1;
[PTBR+15] = "0000";

//Stack
[PTBR+16] = 81;
[PTBR+17] = "0110";
[PTBR+18] = 83;
[PTBR+19] = "0000";

//Setting User Area Page for IDLE Process
[PROCESS_TABLE + 1] = 0;
[PROCESS_TABLE + 4] = RUNNING;
[PROCESS_TABLE + 11] = 82;
[PROCESS_TABLE + 12] = 0;
[PROCESS_TABLE + 13] = 8*512;
[PROCESS_TABLE + 14] = PAGE_TABLE_BASE;
[PROCESS_TABLE + 15] = 10;
//Set current PID
[SYSTEM_STATUS_TABLE + 1] = 0;
//Transfer entry point from header of IDLE to stack
[81*512] = [69*512 + 1];
//Stack Pointer to start of stack
SP = 8*512;
ireturn;
